# === Naestro single-node DGX Spark (desktop) ===
NAESTRO_DEPLOYMENT=single_node
GPU_POLICY=tensorrt_llm
LOCAL_MODELS=llama-3.1-70b-fp8,deepseek-v3.2-32b,qwen-3-32b-awq
CONCURRENCY_MAX=8
WS_HEARTBEAT_MS=20000

# Providers & endpoints
PROVIDERS_YAML=./config/providers.yaml

# Memory graph (Graphiti)
GRAPHITI_URL=http://127.0.0.1:9090
GRAPHITI_API_KEY=replace_me

# Cloud API keys (optional spillover / long-context)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# Auth
NAESTRO_JWT_SECRET=replace_me

# Metrics/observability
METRICS_EXPORT=otel

